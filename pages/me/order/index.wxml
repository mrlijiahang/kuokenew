<!--pages/me/order/index.wxml-->
<view class='content'>
  <view class='cell' wx:for='{{dataSource}}' wx:key='index'>
    <image class='cell-image' src='../../../images/home/history.png'></image>
    <text class='cell-title' style='color:black'>{{item.cname}}</text>
    <text class='cell-desc'>{{item.create_time}}</text>
  </view>
</view>